{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "height": 300,
    "title": "Different types of valcano in different regions",
    "data": {
    "url": "data/volcano.csv"
    },
    "mark": "bar",
    "encoding": {
    "x": {
    "field": "region",
    "type": "nominal",
    "title": "Regions",
    "axis":{"labelAngle":30}
    },
    "y": {
        "aggregate":"count",
        "type":"quantitative",
    "title":"number of volcanos"
    },
    "color": {
    "field": "primary_volcano_type",
    "type": "nominal"
    }
    }
   }
   